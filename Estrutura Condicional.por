programa {
  funcao inicio() {
    // Variaveis.
    inteiro valor

    // Solicitação.
    escreva("Digite o valor desejado: ")
    leia(valor)

    // Exibindo Resultados.
    se (valor > 10) {
    escreva("É MAIOR QUE 10!")
    }
    se (valor == 10) {
      escreva("O NUMERO É IGUAL A 10.")
    }
    se (valor < 10) {
    escreva("NÃO É MAIOR QUE 10!")
    } 
    escreva("\n=== FIM DO PROGRAMA ===")

  }
}
